<p id="notice"><%= notice %></p>

<h1>Listing M Lmodels</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @m_lmodels.each do |m_lmodel| %>
      <tr>
        <td><%= link_to 'Show', m_lmodel %></td>
        <td><%= link_to 'Edit', edit_m_lmodel_path(m_lmodel) %></td>
        <td><%= link_to 'Destroy', m_lmodel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New M lmodel', new_m_lmodel_path %>
